prefix=/usr/local
exec_prefix=${prefix}
libdir=/usr/local/lib
includedir=/usr/local/include

Name: absl_any
Description: Abseil any library
URL: https://abseil.io/
Version: 20230802
Requires: absl_bad_any_cast = 20230802, absl_config = 20230802, absl_core_headers = 20230802, absl_fast_type_id = 20230802, absl_type_traits = 20230802, absl_utility = 20230802
Libs: -L${libdir}  
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
